{
    "version":2,
    "builds": [
        {
            "src":"./index.py",
            "use": "@vercel/python",
            "config":{"runtime":"python3.9.13"}
        }
    ],
    "routes": [
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    {
  "build": {
    "env": {
      "MYSQLCLIENT_CFLAGS": "$(mysql_config --cflags)",
      "MYSQLCLIENT_LDFLAGS": "$(mysql_config --libs)"
    }
  }
}

}
